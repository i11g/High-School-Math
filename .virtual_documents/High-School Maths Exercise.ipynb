def fibonacci(n):
    """
    Create a Fibonachi function with n as a parameter 
    """
    if n <= 0:
        return []
    elif n == 1:
        return [0]
    fibs = [0, 1]
    while len(fibs) < n:
        fibs.append(fibs[-1] + fibs[-2])
    return fibs


