def fib(n):
    """
    Generate the first n Fibonacci numbers as a list using Python buitlins reduce function
    """
    if n <= 0:
        return []
    if n == 1:
        return [0]
    # Start with [0, 1] and then build up to n items
    return reduce(lambda acc, _: acc + [acc[-1] + acc[-2]], range(n - 2), [0, 1])


n = 20
